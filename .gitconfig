[alias]
	co = checkout
	br = branch
	ci = commit
	st = status
	unstage = reset HEAD --
[filter "lfs"]
	clean = git-lfs clean -- %f
	smudge = git-lfs smudge -- %f
	process = git-lfs filter-process
	required = true
[user]
	name = David Lin
	email = david.lin@maxajen.com
[init]
	defaultBranch = main
[push]
	autoSetupRemote = true
	autoSetupRemote = false
